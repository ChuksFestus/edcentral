      "bundleIdentifier": "com.syllabary.edcentral"
    },
    "android": {
      "package": "com.syllabary.edcentral",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA1wzM3waCF1hphdjY_3SoCFwxEIzJ0jno"
        }
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
